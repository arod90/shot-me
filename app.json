{
  "expo": {
    "name": "shot-me",
    "slug": "shot-me",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "shotme",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shotme.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.shotme.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logo-blanco.png",
          "color": "#ffffff",
          "sounds": ["./assets/sounds/noti.mp3"],
          "androidMode": "default",
          "androidCollapsedTitle": "New Notification",
          "iosDisplayInForeground": true
        }
      ]
    ],
    "notification": {
      "icon": "./assets/images/logo-blanco.png",
      "color": "#ffffff",
      "androidMode": "default",
      "androidCollapsedTitle": "New Notification",
      "iosDisplayInForeground": true
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8035dc51-ae72-41a1-9fe3-bb8e2bbb5613"
      }
    },
    "owner": "arod90",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/8035dc51-ae72-41a1-9fe3-bb8e2bbb5613"
    }
  }
}
